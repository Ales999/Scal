<?xml version="1.0"?>
<package >
  <metadata>
    <id>Scal</id>
    <version>$version$</version>
    <title>Scal</title>
    <authors>Frank Quednau</authors>
    <owners>Frank Quednau</owners>
    <licenseUrl>http://www.opensource.org/licenses/Apache-2.0</licenseUrl>
